{
  "name": "noteme",
  "icon": "assets/image.png",
  "displayName": "Dev Notes",
  "description": "Tracks TODOs, FIXMEs, and build errors, providing reminders in VS Code. Reminder: Future updates may include a sidebar panel, notifications, and task management for better tracking.",
  "version": "1.1.4",
  "publisher": "EpphremM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EpphremM/noteme.git"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:noteme.openview",
    "onView:noteme.openview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "noteme-activitybar",
          "title": "Note Me",
          "icon": "assets/image.png"
        }
      ]
    },
    "views": {
      "noteme-activitybar": [
        {
          "type": "webview",
          "id": "noteme.openview",
          "name": "View",
          "contextualTitle": "View"
        }
      ]
    },
    "commands": [
      {
        "command": "noteme.openview",
        "title": "noteme view "
      },
      {
        "command": "noteme.menu.view",
        "category": "noteme reminder for tracking bugs and tasks",
        "title": " noteme reminder",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "noteme.menu.view",
          "group": "navigation",
          "when": "view == noteme.openview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.22.0",
    "glob": "^11.0.1",
    "mocha": "^11.1.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/minimatch": "^5.1.2",
    "minimatch": "^10.0.1"
  }
}
