{
  "name": "vscode-extension-sidebar-html",
  "displayName": "Sidebar Html VS Code Extension",
  "description": "Sidebar Html VS Code Extension",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
 
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:noteme.openview",
    "onView:noteme.openview"
  ],
  "main": "./out/extension.js",




  
  "contributes": {
    "viewsContainers": { 
      "activitybar": [
        {
          "id": "noteme-activitybar",
          "title": "Note Me",
          "icon": "src/assets/logo.svg"
        }
      ]
    },
    "views": {
      "noteme-activitybar": [
        {
          "type": "webview",
          "id": "noteme.openview",
          "name": "View",
          "contextualTitle": "View"
        }
      ]
    },
    "commands": [
      {
        "command": "noteme.openview",
        "title": "Sidebar View"
      },
      {
        "command": "noteme.menu.view",
        "category": "vscode-extension-sidebar-html",
        "title": "Sample WebView in VS Code Sidebar",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "noteme.menu.view",
          "group": "navigation",
          "when": "view == noteme.openview"
        }
      ]
    }
  },  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.61.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "@vscode/test-electron": "^1.6.2"
  }
}
